{
	"page": {
		"modules": [
			~for m in ms
				{
					"key": {{m.id}},
					"category": "{{m.category}}",
					~if m.text
					"text": "{{m.text}}",
					~endif
					~if m.des
					"text": "{{m.des}}",
					~endif
					~if m.rank
					"rank": "{{m.rank}}"
					~endif
					~if mc[m.id]:
					,
					"items": [
						~for c in mc[m.id]:
						{
							"id": {{c.id}},
							"pic": {
								"id": {{c.pic.id}},
								"min": "{{c.pic.d_min}}",
								"normal": "{{c.pic.d_normal}}",
								"max": "{{c.pic.d_max}}",
								"title": "{{c.pic.title}}",
								"good": {{c.pic.good}},
								"bad": {{c.pic.bad}}
							},
							"component": {
								"id": {{c.component.id}},
								"name": "{{c.component.name}}",
								"thumb": "{{c.component.thumb_http}}",
								"des": "{{c.component.description}}"
							},
							"category": {{c.category}}
						}
						~if loop.index!=loop.length
						,
						~endif
						~endfor
					]
					~endif
				}
				~if loop.index!=loop.length
				,
				~endif
			~endfor
		]
	}
}